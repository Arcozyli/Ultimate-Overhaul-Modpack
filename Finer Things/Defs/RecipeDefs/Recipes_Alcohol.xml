<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
	<defName>DistillStrawberryWine</defName>
	<label>Distill Strawberry Wine</label>
	<description>Distills a bottle of Strawberry Wine.</description>
	<jobString>Distilling Strawberry Wine.</jobString>
	<workSpeedStat>BrewingSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
          <soundWorking>Recipe_Brewing</soundWorking>
          <allowMixingIngredients>true</allowMixingIngredients>
	<workAmount>1500</workAmount>
          <targetCountAdjustment>1</targetCountAdjustment>
	<ingredients>
	  <li>
	    <filter>
	      <thingDefs>
	        <li>RawBerries</li>
	      </thingDefs>
	    </filter>
	    <count>25</count>
	  </li>
	  <li>
	    <filter>
	      <thingDefs>
	        <li>Sugar</li>
	      </thingDefs>
	    </filter>
	    <count>25</count>
	  </li>
	  <li>
	    <filter>
	      <thingDefs>
	        <li>RawHops</li>
	      </thingDefs>
	    </filter>
	    <count>10</count>
	  </li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
	    <li>RawBerries</li>
		<li>Sugar</li>
		<li>RawHops</li>
	  </thingDefs>
	</fixedIngredientFilter>
	<products>
	  <strawberrywine>12</strawberrywine>
	</products>
	<workSkill>Cooking</workSkill>
</RecipeDef>

