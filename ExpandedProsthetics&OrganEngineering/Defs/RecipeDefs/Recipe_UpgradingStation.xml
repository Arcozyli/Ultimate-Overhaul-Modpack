   <RecipeDef ParentName="RecipeAdvancedProsthesis">
      <defName>CreateAdvancedBionicEye</defName>
      <label>Upgrade bionic eye</label>
      <description>Upgrades a bionic eye with vancidium.</description>
      <jobString>Upgrading bionic.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>BionicEye</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
		 <li>
            <filter>
               <thingDefs>
                  <li>Biomatter</li>
               </thingDefs>
            </filter>
            <count>15</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Vancidium</li>
               </thingDefs>
            </filter>
            <count>10</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>BionicEye</li>
			<li>Biomatter</li>
            <li>Vancidium</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AdvancedBionicEye>1</AdvancedBionicEye>
      </products>
   </RecipeDef>
   
