<?xml version="1.0" encoding="utf-8" ?>
<Buildings>

<ThingDef Name="BuildingBase" Abstract="True">
    <category>Building</category>
    <thingClass>Building</thingClass>
    <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <selectable>true</selectable>
    <drawerType>MapMeshAndRealTime</drawerType>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <repairEffect>Repair</repairEffect>
  </ThingDef>

	
	<ThingDef ParentName="BuildingBase">
		<defName>ClutterFloorLampA</defName>
		<label>Floor lamp</label>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
		<texPath>Clutter/Lamp/FloorLamp</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
        <offset>(0,0,-0.1)</offset>
		</shadowData>
		</graphicData>
		<minifiedDef>MinifiedFurniture</minifiedDef>
		<altitudeLayer>BuildingTall</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<tradeTags>
			<li>Furniture</li>
		</tradeTags>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToMake>150</WorkToMake>
			<Flammability>1.0</Flammability>
			<Beauty>5</Beauty>
			<SellPriceFactor>0.25</SellPriceFactor>
		</statBases>
		<selectable>true</selectable>
		<description>
Simple floor lamp</description>
		<size>(1,1)</size>
		<costList>
			<SteelBar>15</SteelBar>
			<CopperWire>5</CopperWire>
			<Glass>5</Glass>
		</costList>
		<soundImpactDefault>BulletImpactMetal</soundImpactDefault>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<comps>
			<li>
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>75</basePowerConsumption>
				<shortCircuitInRain>true</shortCircuitInRain>
				<soundPowerOn>PowerOnSmall</soundPowerOn>
				<soundPowerOff>PowerOffSmall</soundPowerOff>
			</li>
			<li>
				<compClass>CompGlower</compClass>
				<glowRadius>12</glowRadius>
				<glowColor>(217,217,217,0)</glowColor>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
		<designationCategory>Lighting</designationCategory>
		<rotatable>true</rotatable>
		<researchPrerequisite>PowerI</researchPrerequisite>
	</ThingDef>
	
